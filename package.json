{
  "name": "umm-cli",
  "version": "1.0.0",
  "description": "Unity Module Manager CLI",
  "bin": {
    "umm": "./index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps inline",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "test": "mocha --compilers js:babel-register test/*.js"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:monry/umm-cli.git"
  },
  "keywords": [
    "Unity",
    "CLI"
  ],
  "author": "Tetsuya Mori",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monry/umm-cli/issues"
  },
  "homepage": "https://github.com/monry/umm-cli#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "figlet": "^1.2.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "preferences": "^0.2.1",
    "rx": "^4.1.0",
    "simple-git": "^1.73.0",
    "touch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.24.1",
    "power-assert": "^1.4.4"
  }
}
